<?xml version="1.0" ?>
<Simulation>
  <RunInfo>
    <JobName>sauq</JobName>
    <Sequence>sample, write</Sequence>
    <WorkingDir>sauq_runs</WorkingDir>
    <batchSize>1</batchSize>
  </RunInfo>

  <Files>
    <Input name="sauq" type="">../../../model/sauq.m</Input>
    <Input name="rt" type="">../../../model/RateTheory.m</Input>
    <Input name="kc" type="">../../../model/KlemensCallawayModel.m</Input>
  </Files>

  <Models>
    <Code name="fp" subType="GenericCode">
      <!-- local matlab -->
      <!-- <executable>/Applications/MATLAB_R2017b.app/bin/matlab</executable> -->
      <!-- hpc matlab: module load matlab/R2023b-gcc-12.3.0-buub -->
      <executable>matlab</executable>
      <shell>False</shell>
      <outputFile>out_sauq.csv</outputFile>
      <inputExtensions>.m</inputExtensions>
      <clargs arg='-nosplash -nodesktop -r "run ./sauq.m"' type="input" extension='.m'/>
    </Code>
    <PostProcessor name="stats" subType="BasicStatistics" verbosity="debug">
      <skewness prefix="skew">RL, NL, k</skewness>
      <variationCoefficient prefix="vc">RL, NL, k</variationCoefficient>
      <percentile prefix="percentile" interpolation="midpoint">RL, NL, k</percentile>
      <expectedValue prefix="mean">RL, NL, k</expectedValue>
      <kurtosis prefix="kurt">RL, NL, k</kurtosis>
      <median prefix="median">RL, NL, k</median>
      <maximum prefix="max">RL, NL, k</maximum>
      <minimum prefix="min">RL, NL, k</minimum>
      <samples prefix="samp">RL, NL, k</samples>
      <variance prefix="var">RL, NL, k</variance>
      <sigma prefix="sigma">RL, NL, k</sigma>
      <NormalizedSensitivity prefix="nsen">
        <targets>RL, NL, k</targets>
        <features>damage</features>
      </NormalizedSensitivity>
      <sensitivity prefix="sen">
        <targets>RL, NL, k</targets>
        <features>damage</features>
      </sensitivity>
      <pearson prefix="pear">
        <targets>RL, NL, k</targets>
        <features>damage</features>
      </pearson>
      <covariance prefix="cov">
        <targets>RL, NL, k</targets>
        <features>damage</features>
      </covariance>
      <VarianceDependentSensitivity prefix="vsen">
        <targets>RL, NL, k</targets>
        <features>damage</features>
      </VarianceDependentSensitivity>
    </PostProcessor>
  </Models>

  <Distributions>
    <Uniform name="damage_dist">
      <lowerBound>0.0</lowerBound>
      <upperBound>1.0</upperBound>
    </Uniform>
  </Distributions>

  <Samplers>
    <MonteCarlo name="MC">
      <samplerInit>
        <limit>10</limit>
      </samplerInit>
      <variable name="damage">
        <distribution>damage_dist</distribution>
      </variable>
    </MonteCarlo>
  </Samplers>

  <Steps>
    <MultiRun name="sample" clearRunDir="False">
      <Input class="Files" type="">sauq</Input>
      <Input class="Files" type="">rt</Input>
      <Input class="Files" type="">kc</Input>
      <Model class="Models" type="Code">fp</Model>
      <Sampler class="Samplers" type="MonteCarlo">MC</Sampler>
      <Output class="DataObjects" type="HistorySet">samples</Output>
    </MultiRun>
    <IOStep name="write">
      <Input class="DataObjects" type="HistorySet">samples</Input>
      <Output class="OutStreams" type="Print">dump_samples</Output>
    </IOStep>
    <PostProcess name="stats">
      <Input class="DataObjects" type="HistorySet">samples</Input>
      <Model class="Models" type="PostProcessor">stats</Model>
      <Output class="DataObjects" type="HistorySet">stats_do</Output>
      <Output class="OutStreams" type="Print">stats_dump</Output>
    </PostProcess>
  </Steps>

  <DataObjects>
    <HistorySet name="samples">
      <Output>irrT, RL, NL, k</Output>
      <options>
        <pivotParameter>irrT</pivotParameter>
      </options>
    </HistorySet>
    <HistorySet name="stats_do">
      <Output>
        skew_RL
        skew_NL
        skew_k
        vc_RL
        vc_NL
        vc_k
        percentile_5_RL
        percentile_5_NL
        percentile_5_k
        percentile_95_RL
        percentile_95_NL
        percentile_95_k
        mean_RL
        mean_NL
        mean_k
        kurt_RL
        kurt_NL
        kurt_k
        median_RL
        median_NL
        median_k
        max_RL
        max_NL
        max_k
        min_RL
        min_NL
        min_k
        samp_RL
        samp_NL
        samp_k
        var_RL
        var_NL
        var_k
        sigma_RL
        sigma_NL
        sigma_k
        nsen_RL_damage
        nsen_NL_damage
        nsen_k_damage
        sen_RL_damage
        sen_NL_damage
        sen_k_damage
        pear_RL_damage
        pear_NL_damage
        pear_k_damage
        cov_RL_damage
        cov_NL_damage
        cov_k_damage
        vsen_RL_damage
        vsen_NL_damage
        vsen_k_damage
      </Output>
      <options>
        <pivotParameter>irrT</pivotParameter>
      </options>
    </HistorySet>
  </DataObjects>

  <OutStreams>
    <Print name="dump_samples">
      <type>csv</type>
      <source>samples</source>
      <what>input, output</what>
    </Print>
    <Print name="stats_dump">
      <type>csv</type>
      <source>stats_do</source>
      <what>input, output</what>
    </Print>
  </OutStreams>

</Simulation>
